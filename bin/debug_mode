#!/bin/bash

TMPDIR=/tmp/spacemacs-mmap xpra start $DISPLAY\
  --compress="${XPRA_COMPRESS}"\
  --debug=all\
  --dpi="${XPRA_DPI}"\
  --encoding="${XPRA_ENCODING}"\
  --keyboard-sync="${XPRA_KEYBOARD_SYNC}"\
  --mmap="${XPRA_MMAP}"\
  --no-daemon\
  --pulseaudio=no\
  --sharing="${XPRA_SHARING}"\
  --start-child="$@"
